pipeline {
    agent any
    triggers { upstream(upstreamProjects: 'pipeline', threshold: hudson.model.Result.SUCCESS) }
    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
